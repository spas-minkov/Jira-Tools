{
    "manifest_version": 3,
    "name": "Jira-Tools",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://jira.tinqin.com/*",
                "https://jira.viamedis.fr/*"
            ],
            "js": ["content.js"],
            "match_origin_as_fallback": true
        }
    ]
}